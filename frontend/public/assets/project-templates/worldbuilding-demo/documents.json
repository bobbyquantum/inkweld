            { "type": "elementRef", "attrs": { "elementId": "char-theron", "elementType": "WORLDBUILDING", "displayText": "Theron", "originalName": "Theron Blackwood", "relationshipTypeId": "referenced-in" } },
            { "type": "text", "text": " formed an unlikely friendship when he helped her navigate " },
            { "type": "elementRef", "attrs": { "elementId": "loc-thornwood", "elementType": "WORLDBUILDING", "displayText": "Thornwood", "originalName": "Thornwood Forest", "relationshipTypeId": "referenced-in" } },
