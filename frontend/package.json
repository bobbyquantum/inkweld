{
  "name": "inkweld-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:prod": "ng serve --configuration production",
    "clean": "rimraf dist",
    "build": "ng build",
    "compress": "node scripts/compress.js",
    "watch": "ng build --watch --configuration development",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "analyze": "ng build --stats-json && esbuild-visualizer --metadata dist/inkweld-frontend/stats.json",
    "upgrade-angular": "ng update @angular/core@20 @angular/cli@20 @angular/material@20 @angular/cdk@20 --allow-dirty",
    "fix-angular-control-flow": "ng generate @angular/core:control-flow",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.0.2",
    "@angular/cdk": "20.0.2",
    "@angular/common": "20.0.2",
    "@angular/compiler": "20.0.2",
    "@angular/core": "20.0.2",
    "@angular/forms": "20.0.2",
    "@angular/material": "20.0.2",
    "@angular/platform-browser": "20.0.2",
    "@angular/platform-browser-dynamic": "20.0.2",
    "@angular/router": "20.0.2",
    "@angular/service-worker": "20.0.2",
    "@progress/jszip-esm": "1.0.4",
    "angular-split": "^19.0.0",
    "date-fns": "^4.0.0",
    "vitest-mock-extended": "^3.1.0",
    "lodash-es": "^4.17.21",
    "nanoid": "5.1.5",
    "ngx-editor": "18.0.0",
    "ngx-image-cropper": "9.1.5",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "y-indexeddb": "9.0.12",
    "y-prosemirror": "1.3.5",
    "y-websocket": "3.0.0",
    "yjs": "13.6.27",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.0.1",
    "@angular/build": "20.0.1",
    "@angular/cli": "20.0.1",
    "@angular/compiler-cli": "20.0.1",
    "@ngneat/spectator": "^19.4.1",
    "@playwright/test": "1.52.0",
    "@types/lodash-es": "^4.17.12",
    "angular-eslint": "19.8.0",
    "esbuild": "0.25.5",
    "esbuild-visualizer": "0.7.0",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "fake-indexeddb": "6.0.1",
    "@analogjs/vite-plugin-angular": "^1.17.1",
    "@analogjs/vitest-angular": "^1.17.1",
    "vitest": "^3.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "playwright-ng-schematics": "^2.0.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.33.1"
  }
}
