{
  "name": "worm-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:prod": "ng serve --configuration production",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "analyze": "ng build --stats-json && esbuild-visualizer --metadata dist/worm-frontend/stats.json",
    "storybook": "ng run worm-frontend:storybook",
    "build-storybook": "ng run worm-frontend:build-storybook",
    "upgrade-storybook": "npx storybook@latest upgrade",
    "upgrade-angular": "ng update @angular/core@19 @angular/cli@19 @angular/material@19 @angular/cdk@19 --allow-dirty"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.0.5",
