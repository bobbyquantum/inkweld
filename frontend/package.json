{
  "name": "inkweld-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:prod": "ng serve --configuration production",
    "clean": "rimraf dist",
    "build": "ng build",
    "compress": "node scripts/compress.js",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageReporters=lcov",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "analyze": "ng build --stats-json && esbuild-visualizer --metadata dist/inkweld-frontend/stats.json",
    "upgrade-angular": "ng update @angular/core@19 @angular/cli@19 @angular/material@19 @angular/cdk@19 --allow-dirty",
    "fix-angular-control-flow": "ng generate @angular/core:control-flow",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.12",
    "@angular/cdk": "19.2.16",
